{
    "name": "stubbles/bootstrap",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Bootstrap project to create a new stubbles project from",
    "require": {
        "php": ">=5.3.0",
        "stubbles/stubbles-core": "~3.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.1",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "net\\stubbles": "src/main/php"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
